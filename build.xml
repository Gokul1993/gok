<?xml version="1.0"?>

<project name="appedo" default="copy" basedir=".">



<target name="delete">
<delete dir="src"/>
<delete dir="WebContent"/>
<delete dir="build"/>
</target>
 
<target name="init" depends="delete">
<mkdir dir="bin"/>
<mkdir dir="src"/>
<mkdir dir="lib"/>
<mkdir dir="test"/>
</target>
<target name="copy" depends="init">
<copy file="/root/.jenkins/jobs/appedo/workspace/helloworld.java" tofile="root/.jenkins/jobs/appedo/workspace/src"/>
</target>




</project>
